{
  "name": "feat",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "1.0.0",
  "publisher": "bancodobrasil",
  "license": "MIT",
  "engines": {
    "vscode": "*"
  },
  "scripts": {
    "update-grammar": "vscode-grammar-updater bancodobrasil/feat.tmbundle Syntaxes/Feat.plist ./syntaxes/feat.tmLanguage.json"
  },
  "contributes": {
    "languages": [
      {
        "id": "feat",
        "extensions": [
          ".feat"
        ],
        "aliases": [
          "Feat",
          "feat"
        ],
        "configuration": "./feat.language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "feat",
        "scopeName": "source.feat",
        "path": "./syntaxes/feat.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "vscode-grammar-updater": "^1.0.3"
  }
}
