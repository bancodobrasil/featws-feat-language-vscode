{
	"information_for_contributors": [
		"This file has been converted from https://github.com/bancodobrasil/feat.tmbundle/blob/master/Syntaxes/Feat.plist",
		"If you want to provide a fix or improvement, please create a pull request against the original repository.",
		"Once accepted there, we are happy to receive an update request."
	],
	"version": "https://github.com/bancodobrasil/feat.tmbundle/commit/aa1b0fd23c55eab7d9b3b2edc0c29dd5bcf6cc00",
	"name": "Feat",
	"scopeName": "source.feat",
	"patterns": [
		{
			"begin": "(^[ \\t]+)?(?=;)",
			"beginCaptures": {
				"1": {
					"name": "punctuation.whitespace.comment.leading.feat"
				}
			},
			"end": "(?!\\G)",
			"patterns": [
				{
					"begin": ";",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.comment.feat"
						}
					},
					"end": "\\n",
					"name": "comment.line.semicolon.feat"
				}
			]
		},
		{
			"captures": {
				"1": {
					"name": "keyword.other.definition.feat"
				},
				"2": {
					"name": "punctuation.separator.key-value.feat"
				}
			},
			"match": "\\b([a-zA-Z0-9_.-]+)\\b\\s*(=)"
		},
		{
			"captures": {
				"1": {
					"name": "punctuation.definition.entity.feat"
				},
				"3": {
					"name": "punctuation.definition.entity.feat"
				}
			},
			"match": "^(\\[)(.*?)(\\])",
			"name": "entity.name.section.group-title.feat"
		},
		{
			"begin": "'",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.string.begin.feat"
				}
			},
			"end": "'",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.string.end.feat"
				}
			},
			"name": "string.quoted.single.feat",
			"patterns": [
				{
					"match": "\\\\.",
					"name": "constant.character.escape.feat"
				}
			]
		},
		{
			"begin": "\"",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.string.begin.feat"
				}
			},
			"end": "\"",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.string.end.feat"
				}
			},
			"name": "string.quoted.double.feat"
		}
	]
}